<html>

<head>
  <meta charset="utf-8">
  <title>Gutenberg</title>

  <link rel="stylesheet" href="<%= basedir %>/plugins/bootstrap/css/bootstrap.min.css" charset="utf-8">
</head>

<body>
  <div class="bs-docs-section">
    <div class="bs-example" data-example-id="simple-table">
      <table class="table">
        <caption>
          <h4 class="text-center">品质检验单</h4>
          <br>
          <div class="invoice-label">
            <label class="pull-right">NO：<%=ejsData.qualitycheck_id%></label>
          </div>
        </caption>
        <thead>
          <tr>
            <th>物料编码</th>
            <th>物料名称</th>
            <th>规格</th>
            <th>单位</th>
            <th>合格数量</th>
            <th>不合格数量</th>
            <th>备注</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i = 0; i < ejsData.qualityOrderItemList.length; ++i) {%>
            <tr>
              <td><%=ejsData.qualityOrderItemList[i].materiel_code%></td>
              <td><%=ejsData.qualityOrderItemList[i].materiel_name%></td>
              <td><%=ejsData.qualityOrderItemList[i].materiel_format%></td>
              <td><%=ejsData.qualityOrderItemList[i].materiel_unit + ' * ' +ejsData.qualityOrderItemList[i].materiel_format%></td>
              <td><%=ejsData.qualityOrderItemList[i].qualified_number%></td>
              <td><%=ejsData.qualityOrderItemList[i].unqualified_number%></td>
              <td><%=ejsData.qualityOrderItemList[i].remark%></td>
            </tr>
          <% } %>
        </tbody>
      </table>
      <div class="invoice-label">
          <label>签名:&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;</label>
          <label class="pull-right">年&emsp;&emsp;&emsp;&emsp;月&emsp;&emsp;&emsp;&emsp;日</label>
      </div>
    </div>
  </div>
</body>
</html>
